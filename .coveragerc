[paths]
source = src

[run]
branch = true
source =
    src
    tests
parallel = true

[report]
show_missing = true
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if 0:
    if False:
    if __name__ == .__main__.:
omit =
    tests/*
    test/*

